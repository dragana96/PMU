<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.pocketsoccer_pmu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".FieldActivity" />
        <activity android:name=".SelectPlayersActivity" />
        <activity android:name=".SingleOrMultiplayerActivity" />
        <activity android:name=".GameTypeActivity" />
        <activity android:name=".GameOverActivity" />
        <activity android:name=".StatsActivity" />
        <activity android:name=".ContinueActivity" />
        <activity android:name=".SpeedActivity"></activity>
    </application>

</manifest>